/* 登录 */

.page-login{

}
.page-login .container{
    width: 95%;
    margin: 0 auto;
    padding-top: 2.5rem;
}
.page-login .main{
    width: 100%;
    background: #fff;
    border-radius: 0.75rem;
    box-shadow: 0 0 0.45rem rgba(121,121,121,0.2);
    padding: 1.25rem 1rem;
}
.page-login .main .tit{
    border-left: 0.15rem solid #2cd0b5;
    padding-left: 0.5rem;
    font-size: 0.7rem;
    color: #333333;
    margin-bottom: 1rem;
}
.page-login .main .inp-box{
    width: 100%;
    height: 2rem;
    border-radius: 0.2rem;
    border: 1px solid #A3A9BA;
    margin-bottom: 0.5rem;
    padding: 0 0.5rem;
}
.page-login .main .inp-box .left{
    flex: 1;
    margin-right: 0.5rem;
}
.page-login .main .inp-box .icon{
    width: 1rem;
    height: 1rem;
}
.page-login .main .inp-box .i1{
    background-image: url('../images/icon_tel.png');
}
.page-login .main .inp-box .i2{
    background-image: url('../images/icon_eye1.png');
}
.page-login .main .inp-box .i3{
    background-image: url('../images/icon_eye2.png');
}
.page-login .main .inp-box .i4{
    background-image: url('../images/icon_yzcode.png');
}
.page-login .main .inp-box input{
    width: 100%;
    font-size: 0.7rem;
    color: #182952;
}

.page-login .main .tx{
    width: 100%;
    justify-content: space-between;
    padding-top: 1rem;
    color: #979797;
}
.page-login .main .tx a{
    color: #979797;
}
.page-login .main .tx a .sign-up{
    opacity: 1;
    color: #f7776e;
}
.page-login .main .tx a span{
    opacity: 0.5;
}
.page-login .btn-box{
    padding: 2rem 0 1rem;
}
.page-login .btn-box .btn{
    width: 70%;
}



/* 注册 */
.page-login .main .yz-code{
    width: 100%;
}
.page-login .main .yz-code .inp-box{
    flex: 1;
}
.page-login .main .yz-code .code{
    height: 2rem;
    line-height: 2rem;
    vertical-align: middle;
    border-radius: 0.2rem;
    border: 1px solid #A3A9BA;
    margin-bottom: 0.5rem;
    text-align: center;
    font-size: 0.7rem;
    color: #f7776e;
    margin-left: 0.5rem;
    width: 5rem;
}
.page-login .main .yz-code .has{
    color: #8B94A8;
}
.page-login .main .ch{
    width: 100%;
    padding-top: 0.5rem;
    padding-bottom: 0.5rem;
}
.page-login .main .ch input{
    display: none;
}
.page-login .main .ch span{
    width: 0.6rem;
    height: 0.6rem;
    margin-right: 0.25rem;
    border: 1px solid #A4ABBB;
    border-radius: 1px;
}
.page-login .main .ch.on span{
    background: url('../images/icon_dui.png') center no-repeat;
    -webkit-background-size: 100%;
    background-size: 100%;
}




/* 忘记密码 */
.page-forgot .container{
    width: 100%;
    padding-top: 2rem;
}
.page-forgot .container .main{
    padding: 0 0 0 0.5rem;
    border-radius: 0;
}
.page-forgot .main .inp-box{
    width: 100%;
    height: 2.5rem;
    border: none;
    border-bottom: 1px solid #D1D4DC;
    margin-bottom: 0;
}
.page-forgot .main .inp-box .name{
    font-size: 0.7rem;
    margin-right: 0.8rem;
}
.page-forgot .main .inp-box:last-child{
    border-bottom: 0;
}
.page-forgot .main .yz-code{
    border-bottom: 1px solid #D1D4DC;
}
.page-forgot .main .yz-code .inp-box{
    border-bottom: 0;
}
.page-forgot .main .yz-code .code{
    border: none;
    border-left: 1px solid #D1D4DC;
    line-height: 1.2rem;
    height: 1.2rem;
    margin-bottom: 0;
    color: #f7776e;
}
.page-forgot .btn-box{
    padding-top: 4rem;
}







/* 首页 */
.page-home .swiper-pagination-bullet{
    width: 0.75rem;
    height: 0.15rem;
    background: #000;
    opacity: 0.36;
    border-radius: 0;
}
.page-home .swiper-pagination-bullet-active{
    background: #364F6B;
    opacity: 1;
}
.page-home .classify{
    width: 100%;
    background: #fff;
    margin-bottom: 0.5rem;
    flex-wrap: wrap;
    padding: 0.5rem 0;
}
.page-home .classify .box{
    width: 25%;
    text-align: center;
    padding: 0.5rem 0;
}
.page-home .classify .box .img{
    width: 2.1rem;
    height: 2.1rem;
    margin: 0 auto 0.25rem;
}
.page-home .classify .box p{
    color: #1C1C1C;
}

.page-home .main{
    width: 100%;
    background: #fff;
    margin-bottom: 0.5rem;
}
.page-home .main .tit{
    width: 100%;
    padding: 0.5rem 0.75rem;
    justify-content: space-between;
    border-bottom: 1px solid #F1F1F1;
}
.page-home .main .tit .name{
    font-size: 0.85rem;
    padding-left: 0.5rem;
    font-weight: 600;
    position: relative;
}
.page-home .main .tit .name:before{
    position: absolute;
    content: "";
    display: block;
    width: 0.12rem;
    height: 0.8rem;
    background: #7045AF;
    top: 0;
    left: 0;
    bottom: 0;
    margin: auto;
}
.page-home .main .tit .more{
    font-size: 0.65rem;
    color: #182952;
}
.page-home .main .tit .more p{
    margin-right: 0.2rem;
}
.page-home .main .list{
    width: 100%;
    justify-content: space-between;
    flex-wrap: wrap;
    padding: 0.75rem;
}







/* 详情 */
.page-detail .swiper-container{
