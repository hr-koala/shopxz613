/*规格弹窗*/
.page-detail .mask{
    display: block;
}
.page-detail .spec-pop{
    position: fixed;
    width: 100%;
    left: 0;
    bottom: 0;
    background: #fff;
    z-index: 99;
    padding-bottom: 3rem;
    max-height: 75%;
    overflow: auto;
}
.page-detail .spec-pop .close{
    width: 1rem;
    height: 1rem;
    background-image: url('../images/icon_close.png');
    position: absolute;
    top: 0.5rem;
    right: 0.75rem;
}
.page-detail .spec-pop .goods{
    width: 100%;
    padding: 0.5rem 0.75rem;
    margin-bottom: 0.75rem;
}
.page-detail .spec-pop .goods .img{
    width: 3.7rem;
    height: 3.7rem;
    margin-right: 0.5rem;
}
.page-detail .spec-pop .goods .right{
    width: 100%;
    height: 3.7rem;
    justify-content: space-between;
    overflow: hidden;
}
.page-detail .spec-pop .goods .name{
    width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #1C1C1C;
    font-size: 0.7rem;
    padding-right: 1.5rem;
}
.page-detail .spec-pop .goods .price{
    color: #FC5185;
    font-size: 0.85rem;
    width: 100%;
}
.page-detail .spec-pop .list{
    width: 100%;
    padding: 0.25rem 0.75rem;
}
.page-detail .spec-pop .list .box{
    width: 100%;
    align-items: flex-start;
}
.page-detail .spec-pop .list .box .name{
    line-height: 1.5rem;
    color: #232323;
    font-size: 0.7rem;
    margin-right: 0.5rem;
    margin-bottom: 0.5rem;
}
.page-detail .spec-pop .list .box ul{
    flex: 1;
    flex-wrap: wrap;
}
.page-detail .spec-pop .list .box ul li{
    margin-right: 0.5rem;
    margin-bottom: 0.5rem;
    min-height: 1.5rem;
    line-height: 1.4rem;
    border: 1px solid #ABB2C0;
    color: #9EA6B6;
    font-size: 0.7rem;
    border-radius: 0.4rem;
    padding: 0 1rem;
}
.page-detail .spec-pop .list .box ul li.on{
    border-color: #FF81A6;
    color: #FD0054;
}
.page-detail .spec-pop .shul{
    width: 100%;
    padding: 0.5rem 0.75rem;
    justify-content: space-between;
}
.page-detail .spec-pop .shul .name{
    color: #232323;
    font-size: 0.7rem;
}
.page-detail .spec-pop .shul .left p{
    color: #707070;
    -webkit-transform: scale(0.9);
    -ms-transform: scale(0.9);
    -o-transform: scale(0.9);
    transform: scale(0.9);
}





/* 分类 */
.page-classify{
    padding-top: 4.5rem;
    height: 100vh;
}
.page-classify .search{
    background: #e14594;
}
.page-classify .search .search-box{
    background: rgba(255,255,255,0.83);
}
.page-classify .container{
    height: 100%;
}
.page-classify .main{
    width: 100%;
    height: 100%;
}
.page-classify .nav{
    width: 5rem;
    height: 100%;
    overflow: auto;
}
.page-classify .nav ul li{
    width: 100%;
    height: 3.15rem;
    display: flex;
    padding: 0 0.5rem;
    word-break: break-all;
    word-wrap: break-word;
    color: #1C1C1C;
    font-size: 0.75rem;
    position: relative;
}
.page-classify .nav ul li p{
    margin: auto;
}
.page-classify .nav ul li.on{
    color: #2B3595;
    background: #fff;
}
.page-classify .nav ul li.on:before{
    content: "";
    display: block;
    width: 0.2rem;
    height: 90%;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    margin: auto;
    background: #2B3595;
}
.page-classify .goods{
    flex: 1;
    height: 100%;
    overflow: auto;
}
.page-classify .goods .box{
    width: 100%;
    background: #fff;
    padding: 0.5rem;
    margin-bottom: 0.5rem;
}
.page-classify .goods .box .img{
    width: 4.5rem;
    height: 4.5rem;
    margin-right: 0.5rem;
}
.page-classify .goods .box .right{
    flex: 1;
    height: 4.5rem;
    justify-content: space-between;
}
.page-classify .goods .box .name{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    color: #1C1C1C;
    font-size: 0.65rem;
    width: 100%;
    word-break: break-all;
    word-wrap: break-word;
}
.page-classify .goods .box .name span{
    /*display: inline-block;*/
    width: 8.55rem;
    height: 1.8rem;
    font-size: 0.7rem;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0.87rem;
    letter-spacing: 0rem;
    color: #1c1c1c;
    overflow:hidden; /*内容超出宽度时隐藏超出部分的内容 */
    text-overflow: ellipsis;/*文字隐藏后添加省略号*/
}
.page-classify .goods .box .bot{
    width: 100%;
    justify-content: space-between;
}
.page-classify .goods .box .price{
    color: #f7776e;
    font-size: 0.85rem;
}
.page-classify .goods .box .icon-cart{
    width: 1rem;
    height: 1rem;
    background: url('../images/icon_nav3.png') center no-repeat;
    -webkit-background-size: 100%;
    background-size: 100%;
}







