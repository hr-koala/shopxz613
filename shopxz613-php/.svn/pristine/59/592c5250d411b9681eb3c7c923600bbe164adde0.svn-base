/* 确认订单 */
.page-order{
    font-family: "SourceHanSansCN-Normal";
}
.page-order .conter{
    overflow: hidden;
}
.page-order .warning:hover{
    position: relative;
    background-position: 5.7rem;
    transition: 1.2s linear;
}
.page-order .warning{
    width: 100%;
    height: .24rem;
    background-image: repeating-linear-gradient(-45deg,
    #ffffff 2rem,
    #e14594 2rem,
    #e14594 2.4rem,
    #ffffff 2.4rem,
    #ffffff 2.8rem,
    #7045af 2.8rem,
    #7045af 3.2rem,
    #ffffff 3.2rem,
    #ffffff 3.6rem);
    background-size: 5.7rem;
}
.page-order .prompt{
    display: block;
    width: 100%;
    height: 1.5rem;
    background-color: #f9d5e7eb;
    line-height: 1.55rem;
    color: #000;
    text-indent: 2rem;
    overflow: hidden;
    white-space: nowrap;
}
.page-order .prompt p{
    position: absolute;
    left: 100%;
}
.page-order .mask-text{
    float: left;
    width: 10%;
    display: block;
    height: 1.5rem;
    background-color: #fad7e9;
    background-image: url(../images/laba.png);
    background-repeat: no-repeat;
    background-size: 1rem;
    background-position: .6rem .25rem;
    line-height: 1.55rem;
    color: #000;
    text-indent: 2rem;
    overflow: hidden;
    white-space: nowrap;
    position: relative;
    z-index: 10;
}
.page-order .hint{
    -webkit-animation: 25s first-marquee linear infinite normal;
    animation: 25s first-marquee linear infinite normal;
    padding:0 15%;
}
@keyframes first-marquee {
    0% {
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }
    100% {
        -webkit-transform: translate3d(-200%, 0, 0);
        transform: translate3d(-200%, 0, 0);
